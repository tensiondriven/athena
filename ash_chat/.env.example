# Copy this file to .env and fill in your values

# OpenRouter API Key (preferred - get from https://openrouter.ai/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Ollama URL (fallback if OpenRouter not available)
OLLAMA_URL=http://10.1.2.200:11434

# Optional: Other provider keys
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here

# Control whether to use OpenRouter (default: true)
USE_OPENROUTER=true